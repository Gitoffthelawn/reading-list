{
  "manifest_version": 2,

  "name": "Reading List",
  "description": "This extension saves a list of links to pages to read later",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "storage"
  ]
}