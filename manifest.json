{
  "manifest_version": 2,

  "name": "Reading List",
  "description": "This extension saves a list of links to pages to read later",
  "version": "2.2.0",
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "context-menu.js"
    ]
  },
  "permissions": [
    "tabs",
    "https://ajax.googleapis.com/",
    "contextMenus",
    "storage"
  ]
}
